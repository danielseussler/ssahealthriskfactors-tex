@article{paigeDesignModelBasedApproaches2022,
  title = {Design- and {{Model-Based Approaches}} to {{Small-Area Estimation}} in a {{Low-}} and {{Middle-Income Country Context}}: {{Comparisons}} and {{Recommendations}}},
  shorttitle = {Design- and {{Model-Based Approaches}} to {{Small-Area Estimation}} in a {{Low-}} and {{Middle-Income Country Context}}},
  author = {Paige, John and Fuglstad, Geir-Arne and Riebler, Andrea and Wakefield, Jon},
